{
  "name": "@cocalc/static",
  "version": "1.136.7",
  "description": "CoCalc's static frontend Webpack-based build system and framework",
  "main": "./dist-ts/src/index.js",
  "exports": {
    ".": "./dist-ts/src/index.js",
    "./webpack-compiler": "./dist-ts/src/webpack-compiler.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf dist dist-ts node_modules",
    "webpack": "NODE_OPTIONS=--max_old_space_size=8000  NODE_ENV=development npx webpack --progress --color --watch",
    "copy-css": "mkdir -p dist-ts/src && cp -v src/*.css dist-ts/src/ && cp -v src/*.svg dist-ts/src/ && cp -v src/*.html dist-ts/src/",
    "tsc": "pnpm run copy-css && cd src && ../../node_modules/.bin/tsc --watch --pretty --preserveWatchOutput",
    "webpack-prod": "NODE_OPTIONS=--max_old_space_size=8000 NODE_ENV=production npx webpack --progress --color --watch",
    "webpack-measure": "COCALC_OUTPUT=dist-measure MEASURE=true NODE_ENV=development pnpm webpack ",
    "webpack-measure-prod": "COCALC_OUTPUT=dist-prod-measure MEASURE=true pnpm webpack-prod",
    "build": "pnpm run build-dev && ./production-build.py",
    "build-dev": "pnpm run copy-css && cd src && ../../node_modules/.bin/tsc --build ",
    "test": "pnpm exec jest",
    "prepublishOnly": "pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "keywords": [
    "python",
    "sage",
    "sagemath",
    "latex",
    "terminal",
    "web",
    "ide",
    "math",
    "mathematics"
  ],
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE.md",
  "bugs": {
    "url": "https://github.com/sagemathinc/cocalc/issues"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/static",
  "dependencies": {
    "@cocalc/assets": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/cdn": "workspace:*",
    "@cocalc/frontend": "workspace:*",
    "@cocalc/util": "workspace:*"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "1.3.3",
    "@types/jquery": "^3.5.5",
    "@types/node": "^18.16.14",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "assert": "^2.0.0",
    "awaiting": "^3.0.0",
    "bootbox": "^4.4.0",
    "bootstrap": "=3.4.1",
    "bootstrap-colorpicker": "^2.5.3",
    "buffer": "^6.0.3",
    "cjsx-loader": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "coffee-cache": "^1.0.2",
    "coffee-loader": "^3.0.0",
    "coffeescript": "^2.5.1",
    "css-loader": "^7.1.2",
    "entities": "^2.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^2.1.2",
    "html-minify-loader": "^1.4.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^3.0.0",
    "jquery": "^3.6.0",
    "jquery-tooltip": "^0.2.1",
    "jquery-ui": "^1.13.1",
    "jquery-ui-touch-punch": "^0.2",
    "jquery.scrollintoview": "^1.9",
    "json-loader": "^0.5.7",
    "less": "^4.1.1",
    "less-loader": "^11.0.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.0.2",
    "script-loader": "^0.7.2",
    "source-map-loader": "^3.0.0",
    "stream-browserify": "^3.0.0",
