    "@cocalc/backend": "workspace:*",
    "@cocalc/comm": "workspace:*",
    "@cocalc/jupyter": "workspace:*",
    "@cocalc/sync": "workspace:*",
    "@cocalc/sync-server": "workspace:*",
    "@cocalc/terminal": "workspace:*",
    "@cocalc/util": "workspace:*",
    "awaiting": "^3.0.0",
    "diskusage": "^1.1.3",
    "events": "3.3.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.21"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/sync-server",
  "repository": {
    "type": "git",
