{
  "name": "@cocalc/sync-server",
  "version": "0.1.0",
  "description": "CoCalc realtime synchronization framework -- server component",
  "exports": {
    "./server/*": "./dist/server/*.js",
    "./x11": "./dist/x11.js",
    "./monitor/activity": "./dist/monitor/activity/index.js",
    "./monitor/usage": "./dist/monitor/usage/index.js",
    "./monitor/status-and-alerts": "./dist/monitor/status-and-alerts.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "preinstall": "npx only-allow pnpm",
